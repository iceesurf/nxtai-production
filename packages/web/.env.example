VITE_ANTHROPIC_API_KEY=your_anthropic_key_here
VITE_OPENAI_API_KEY=your_openai_key_here

# App Configuration
VITE_APP_NAME=NXT.AI
VITE_APP_VERSION=1.0.0
VITE_SUPPORT_EMAIL=support@nxtai.com